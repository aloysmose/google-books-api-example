{
  "name": "book-search",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://marcusbelcher.github.io/google-books-api-example/",
  "repository": {
    "type": "git",
    "url": "git@github.com:marcusbelcher/google-books-api-example.git"
  },
  "license": "MIT",
  "author": "Marcus Belcher",
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "REACT_APP_VERSION=$npm_package_version react-scripts test",
    "test:debug": "REACT_APP_VERSION=$npm_package_version react-scripts --inspect-brk test --runInBand --no-cache",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "material-design-icons": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.13",
    "@types/material-ui": "^0.21.6",
    "@types/node": "11.11.7",
    "@types/node-sass": "^4.11.0",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "@types/react-lottie": "^1.2.2",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-sonarjs": "^0.3.0",
    "faker": "^4.1.0",
    "jest-canvas-mock": "^2.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.4.5",
    "snyk": "^1.166.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "src/**/**/*.{ts,tsx}",
      "src/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ]
  }
}